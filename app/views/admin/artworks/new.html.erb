<%= form_for [:admin, @art] do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :artist %>
<%= f.text_field :artist %>

<%= f.label :material %>
<%= f.text_field :material %>

<%= f.label :location %>
<%= f.text_field :location %>

<%= f.label :detailed_location %>
<%= f.text_field :detailed_location %>

<%= f.label :notes %>
<%= f.text_field :notes %>

<%= f.submit "Create New Artything"%>

<% end %>

<!DOCTYPE html>
<html>
  <body>
    <%= form_for [@user, @user, @artwork, @favorite] do |f| %>
    <%= f.label :rating %>
    <%= select_tag 'rating', options_for_select([1,2,3,4,5]) %>
    <%= f.label :comment %>
    <%= f.text_field :comment %>
    <%= f.submit 'Create Favorite' %>
    <% end %>
  </body>
</html>
